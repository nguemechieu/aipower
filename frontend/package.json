{
  "name": "frontend",
  "version": "0.2.0",
  "private": true,
  "proxy": "http://localhost:3000",
  "homepage": "https://nguemechieu.github.io/aipower",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nguemechieu/aipower.git"
  },
  "author": "Noel nguemechieu <nguemechieu.github.io>",
  "bugs": {
    "url": "https://github.com/nguemechieu/aipower/issues"
  },
  "description": "A professional-grade React frontend for AIPower.",
  "dependencies": {
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@fortawesome/fontawesome-svg-core": "^6.7.1",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@mui/icons-material": "^6.2.1",
    "@mui/material": "^6.1.10",
    "@reduxjs/toolkit": "^2.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react": "^16.1.0",
    "autolinker": "^4.0.1",
    "axios": "^1.7.8",
    "cross-env": "^7.0.3",
    "csurf": "^1.10.0",
    "dotenv": "^16.4.7",
    "lightweight-charts": "^4.2.1",
    "lint-staged": "^15.2.10",
    "lru-cache": "^11.0.2",
    "node-polyfill-webpack-plugin": "^4.0.0",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.0.2",
    "react-scripts": "^5.0.1",
    "recharts": "^2.13.3",
    "redux-thunk": "^3.1.0",
    "rimraf": "^6.0.1",
    "serve": "^14.2.4",
    "styled-components": "^6.1.13",
    "svgo": "^3.3.2",
    "swagger-ui": "^5.18.2",
    "swagger-ui-react": "^5.18.2",
    "web-vitals": "^4.2.4",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.16.0",
    "gh-pages": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm": "^11.0.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.3",
    "typescript": "^4.9.5",
    "util": "^0.12.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "development": "cross-env NODE_ENV=development npm run start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "analyze": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --profile --json > stats.json",
    "analyze:bundle": "npm run analyze && npx bundle-visualizer stats.json",
    "test": "jest",
    "build": "cross-env NODE_ENV=production react-scripts build",
    "production": "npx serve -s build -l 3000",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss,md,json}'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "airbnb",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [
            ".jsx",
            ".tsx"
          ]
        }
      ],
      "import/prefer-default-export": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
